{
  "name": "media-hub",
  "version": "1.0.0",
  "description": "A comprehensive YouTube-like media sharing platform for videos, music, courses, and educational content",
  "main": "server.js",
  "scripts": {
    "start": "prisma generate && node server.js",
    "start:mysql": "node server-mysql.js",
    "dev": "nodemon server.js",
    "dev:mysql": "nodemon server-mysql.js",
    "build": "echo 'Build completed'",
    "test": "echo 'Run tests here'",
    "setup": "mkdir -p uploads && echo 'Uploads directory created'",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:studio": "prisma studio",
    "db:seed": "node prisma/seed.js",
    "mysql:init": "mysql -u root -p < database/schema.sql",
    "mysql:seed": "mysql -u root -p media_hub < database/seed.sql",
    "mysql:console": "mysql -u root -p media_hub"
  },
  "keywords": [
    "media-platform",
    "video-sharing",
    "youtube-clone",
    "music-platform",
    "educational-content",
    "streaming",
    "content-creation",
    "social-media"
  ],
  "author": "Media Hub Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/developer55733/media-hub.git"
  },
  "dependencies": {
    "express": "^4.18.2",
    "multer": "^2.0.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "helmet": "^7.1.0",
    "express-rate-limit": "^7.1.5",
    "pg": "^8.11.3",
    "@prisma/client": "^5.7.1",
    "prisma": "^5.7.1",
    "mysql2": "^3.6.5",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2",
    "joi": "^17.11.0",
    "cloudinary": "^1.41.0",
    "multer-storage-cloudinary": "^4.0.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.2",
    "eslint": "^8.57.0",
    "prettier": "^3.1.1",
    "jest": "^29.7.0"
  },
  "engines": {
    "node": ">=14.0.0",
    "npm": ">=6.0.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
